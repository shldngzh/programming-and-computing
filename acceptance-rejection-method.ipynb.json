   "source": [
    "Z  = []\n",
    "for i in range(N):\n",
    "    U1 = np.random.rand()\n",
    "    U2 = np.random.rand()\n",
    "    Y1 = -np.log(U1)\n",
    "    Y2 = -np.log(U2)\n",
    "    while Y1 < (Y2-1)**2/2-1/2:\n",
    "        U1 = np.random.rand()\n",
    "        U2 = np.random.rand()\n",
    "        Y1 = -np.log(U1)\n",
    "        Y2 = -np.log(U2)\n",
    "    U = np.random.rand()\n",
    "    if U<=0.5:\n",
    "        Z.append(Y1)\n",
    "    else:\n",
    "        Z.append(-Y2)\n",
    "\n",
    "Z = np.array(Z).reshape((5000,1))\n",
    "\n",
    "Z_std = np.random.randn(5000,1)\n",
    "Z_std.shape, Z.shape\n",
    "df = pd.DataFrame(np.hstack([Z, Z_std]), columns=['generated', 'standard'])\n",
    "\n",
    "\n",
    "plt.figure(figsize=(20, 20*.618))\n",
    "df.plot(kind='kde')\n",
    "\n",
    "pass"
   ]
  }
