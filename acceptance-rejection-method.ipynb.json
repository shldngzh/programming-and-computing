   "source": [
    "N = 5000\n",
    "Z = []\n",
    "for i_ in range(N):\n",
    "    U = np.random.rand()\n",
    "    Y = -np.log(U) \n",
    "    U = np.random.rand()\n",
    "    while U > np.exp(-(Y-1)**2/2+1/2):\n",
    "        U = np.random.rand()\n",
    "        Y = -np.log(U)    \n",
    "    Z_abs = Y\n",
    "    U = np.random.rand()\n",
    "    if U<=0.5:\n",
    "        Z.append(Z_abs)\n",
    "    else:\n",
    "        Z.append(-Z_abs)\n",
    "        \n",
    "Z = np.array(Z).reshape((5000,1))\n",
    "\n",
    "Z_std = np.random.randn(5000,1)\n",
    "Z_std.shape, Z.shape\n",
    "\n",
    "df = pd.DataFrame(np.hstack([Z, Z_std]), columns=['generated', 'standard'])\n",
    "plt.figure()\n",
    "df.plot(kind='kde')\n",
    "\n",
    "pass"
   ]
